{
  "name": "automation-qa-cypress",
  "version": "1.0.0",
  "type": "module",
  "description": "Repo for using the Cypress framework to create E2E tests",
  "homepage": "https://github.com/karapuz1337/automation-qa-cypress#readme",
  "bugs": {
    "url": "https://github.com/karapuz1337/automation-qa-cypress/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karapuz1337/automation-qa-cypress.git"
  },
  "license": "ISC",
  "author": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:headed": "cypress run --headed",
